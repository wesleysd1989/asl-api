{
  "name": "asl-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:server": "nodemon src/server.js",
    "dev:queue": "nodemon src/queue.js",
    "debug": "nodemon --inspect src/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "db:create": "sequelize db:migrate && sequelize db:seed:all",
    "db:undo": "sequelize db:migrate:undo:all",
    "production:server": "PM2_LOG_FILE_PATH=/dev/null pm2 start dist/server.js -o '/dev/null' -e '/dev/null'",
    "production:queue": "PM2_LOG_FILE_PATH=/dev/null pm2 start dist/queue.js -o '/dev/null' -e '/dev/null'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "5.10.2",
    "bcryptjs": "^2.4.3",
    "bull": "^3.13.0",
    "bull-board": "^0.7.0",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.14",
    "multer": "^1.4.2",
    "nodemailer": "^6.2.1",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.4.0",
    "provable": "0.2.3",
    "puppeteer": "^4.0.0",
    "random-js": "1.0.8",
    "sequelize": "5.9.4",
    "uuid": "^8.2.0",
    "youch": "^2.0.10",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^25.2.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.2.7",
